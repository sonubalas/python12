  print("intiger")

num=int(input("enter the age"))
if(num<0):
  print("negetive")
elif(num>0):
  print("positive")


else:
  print("zero")

x=20
y=10
if(x<y):
  print("x is smaller")
elif(x>y):
  print("y is bigger")

else:
  print("none")


x=10
y=20
z=15
if(x<y and x<z):
  print("smaller")
elif(y<x and y<z):
  print("bigger")
elif(z<x and z<y):
  print("equal")
else:
  print("none")

x=int(input("enter a number"))
if(x%2==0):
  print("even")
else:
  print("odd")

age=int(input("enter age"))
if(age>10 and age<18):
  print("child")
elif(age>=18 and age<40):
  print("young")
elif(age>=40 and age<60):
  print("senior")
else:
  print("citizen")